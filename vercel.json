{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/redirect/(.*)$",
      "headers": {
        "X-Robots-Tag": "noindex, nofollow"
      },
      "continue": true
    },
    {
      "src": "/api/([^\\.]+)$",
      "dest": "/api/$1.js"
    }
  ]
}