{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public_html/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/([^\\.]+)$",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public_html/$1"
    }
  ],
  "cleanUrls": true
}
