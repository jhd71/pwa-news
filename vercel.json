{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public_html/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/([^\\.]+)$",
      "dest": "/api/$1.js"
    },
    {
      "src": "/index.html",
      "dest": "/"
    },
    {
      "src": "/meteo.html",
      "dest": "/meteo"
    },
    {
      "src": "/meteo",
      "dest": "/public_html/meteo.html"
    },
    {
      "src": "/(.*)\\.html$",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public_html/$1"
    }
  ],
  "cleanUrls": true
}
